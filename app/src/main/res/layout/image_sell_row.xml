<?xml version="1.0" encoding="utf-8"?>
<com.example.user.banhangonline.views.SquareFrameLayoutSell xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/img_sell_row"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:padding="2dp">

    <com.example.user.banhangonline.views.SquareImageView
        android:id="@+id/img_library"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

    <FrameLayout
        android:layout_width="32dp"
        android:layout_height="32dp"
        android:layout_gravity="end"
        android:layout_marginEnd="@dimen/pading_small"
        android:layout_marginTop="@dimen/pading_small">

        <com.example.user.banhangonline.views.CircleView
            android:id="@+id/circle"
            android:layout_width="32dp"
            android:layout_height="32dp" />

        <ImageView
            android:id="@+id/check"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:src="@drawable/ic_check" />

    </FrameLayout>
</com.example.user.banhangonline.views.SquareFrameLayoutSell>